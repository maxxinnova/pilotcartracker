{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "typeRoots": [
      "/node_modules/@types"
    ],
    "strict": false,
    "alwaysStrict": false,
    "noImplicitUseStrict": true
  },
  "exclude": [
    "node_modules"
  ]
}
